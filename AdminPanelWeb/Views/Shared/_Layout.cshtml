@using Microsoft.AspNetCore.Http
@using System.Text.Json
@inject IHttpContextAccessor HttpContextAccessor
@{
    User user = new User();
    if (HttpContextAccessor.HttpContext.Session.GetString("CurrentUser") != null && HttpContextAccessor.HttpContext.Session.GetString("CurrentUser") != "")
    {
        user = JsonSerializer.Deserialize<User>(HttpContextAccessor.HttpContext.Session.GetString("CurrentUser"));
    }
}
